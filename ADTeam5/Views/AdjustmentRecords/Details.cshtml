@model List<ADTeam5.ViewModels.AdjustmentRecordDetails>

@{
    ViewData["Title"] = "Details";
}

@*<h1>Items to be added to voucher</h1>*@
<section class="content">
    <div class="row">
        <div class="col-lg-12">
            <div class="box">
                <div class="box-header">
                    <div align="center" vertical-align="middle">
                        <div style="width: 400px;padding: 20px;margin: 20px;border-style:outset; border-width:1px; ">
                            <div align="center" vertical-align="middle">
                                <h1 class="box-title">
                                    <strong><font size="5">Adjustment Details</font></strong>
                                </h1>
                            </div>
                            <div class="box-tools">
                            </div>
                        </div>
                    </div>


                    <div class="container">
                        <div class="row">
                            <div class="col">
                                <div id="TempVoucherDetailsList">
                                    <partial name="_TempDetails" model="Model" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

