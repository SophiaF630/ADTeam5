@model IEnumerable<ADTeam5.ViewModels.ViewRequestDetails>

@{
    ViewData["Title"] = "Details";
}


<h2>Details</h2>

<p><b>Requisition Form No: @ViewData["RRID"]</b></p>
<p>Status: @ViewData["Status"]</p>


<table class="table">
    <thead>
        <tr>
            <th>
                Item Name
            </th>
            <th>
                Quantity
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.itemName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.quantity)
            </td>

        </tr>
        }
    </tbody>
</table>

<div>
    <a asp-action="Index">Back to List</a>
</div>

